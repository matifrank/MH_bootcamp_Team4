      "source": [
        " #  Generamos Query y join the dos tablas para crear una nueva de resultados con datos agregados\n",
        "def load_table_from_bq(query):\n",
        "    return pd.read_gbq(query, project_id=project_id)\n",
        "\n",
        "table1 = 'games_details'\n",
        "table2 = 'teams'\n",
        "\n",
        "query = f\"\"\"\n",
        "SELECT t2.ABBREVIATION, t2.NICKNAME,\n",
        "COUNT(t1.GAME_ID) AS games_played\n",
        "FROM `{project_id}.{dataset}.{table1}` AS t1\n",
        "INNER JOIN `{project_id}.{dataset}.{table2}` AS t2\n",
        "ON t1.TEAM_ID = t2.TEAM_ID\n",
        "GROUP BY t2.ABBREVIATION, t2.NICKNAME\n",
        "ORDER BY games_played DESC;\n",
        "\"\"\"\n",
        "\n",
        "merged_df = load_table_from_bq(query)"
